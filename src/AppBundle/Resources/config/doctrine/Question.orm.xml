<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Question" table="question">
    <id name="idquestion" type="integer" column="idquestion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="text" type="text" column="text" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="evaluation" type="integer" column="evaluation" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="testtest" target-entity="Test" mapped-by="questionquestion" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
